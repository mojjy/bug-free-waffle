{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "rootDir": "./src",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "typeRoots": [ "./node_modules/@types" ],
    "sourceMap": true,
    "types": [ "jasmine", "node", "core-js", "protractor"]
  },
  "exclude": [
    "node_modules"
  ]
}
